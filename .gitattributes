  'http://localhost:8080/WeatherAdvice' \
  -H 'accept: application/json' \
  -H 'Content-Type: application/json' \
  -d '{
  "Weather": {
    "rainForecast": 0,
    "stormAlert": true,
    "temperature": "cold"
  },
  "Name": "string"
}'
